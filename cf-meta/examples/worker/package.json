{
  "name": "@cf-meta/example-worker",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "check": "tsc --noEmit",
    "build:go": "CGO_ENABLED=0 GOOS=linux GOARCH=amd64 go build -tags nocompress,nosqlite,nomysql,nopg,notikv,nobadger,noetcd -o ../../../juicefs-cfmount ../../../cmd/cfmount",
    "build:go:wasm": "CGO_ENABLED=0 GOOS=wasip1 GOARCH=wasm go build -tags nocompress,nosqlite,nomysql,nopg,notikv,nobadger,noetcd -o ../../../juicefs-cfmount.wasm ../../../cmd/cfmount",
    "dev": "wrangler dev",
    "deploy": "pnpm run build:go && wrangler deploy",
    "test:e2e": "tsx test-e2e.ts"
  },
  "dependencies": {
    "@cf-meta/wskv-server": "workspace:*",
    "@cloudflare/sandbox": "^0.7.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4",
    "tsx": "^4.21.0",
    "typescript": "^5",
    "wrangler": "^4"
  }
}
